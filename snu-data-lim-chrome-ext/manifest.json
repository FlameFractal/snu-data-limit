{
    "name": "SNUDataLimit",
    "version": "0.0.1",
    "manifest_version": 2,
    "browser_action": {
    	"default_icon": {
	        "19": "icons/19x19.png"
    	},
        "default_title": "That's the tool tip",
        "default_popup": "popup.html"
    },

    "content_scripts": [ {
    "js": [ "jquery-2.1.4.min.js" ],
    "matches": [ "http://*/*", "https://*/*"]
    }],

    "background": {
	    "scripts": ["jquery-2.1.4.min.js", "background.js"],
	    "persistent": true
	}

	
}